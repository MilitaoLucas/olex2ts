{
  "grammars": [
    {
      "name": "olex2htm",
      "camelcase": "OLEX2HTM",
      "scope": "source.html",
      "path": ".",
      "external-files": [
        "src/tag.h"
      ],
      "file-types": [
        "html",
        "htm"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "injection-regex": "html"
    }
  ],
  "metadata": {
    "version": "0.23.2",
    "license": "MIT",
    "description": "HTML grammar for tree-sitter",
    "authors": [
      {
        "name": "Max Brunsfeld",
        "email": "maxbrunsfeld@gmail.com"
      },
      {
        "name": "Amaan Qureshi",
        "email": "amaanq12@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-html"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": false,
    "swift": true
  }
}
